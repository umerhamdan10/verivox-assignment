{
  "name": "cucumber-puppeteer-typescript-starter",
  "version": "0.0.0",
  "description": "Project starter for using puppeteer with cucumber-js, written in TypeScript",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "lint": "eslint \"support/**/*.ts\" --fix",
    "test": "cucumber-js -f json:report/report.json && node report.js",
    "only": "cucumber-js --tags @only  --parallel 1 -f json:report/report.json && node report.js",
    "generate:report": "node report.js"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@cucumber/cucumber": "^8.10.0",
    "@cucumber/pretty-formatter": "1.0.0",
    "@types/chai": "^4.2.9",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.5.3",
    "@types/puppeteer": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "chai": "^4.2.0",
    "cucumber-html-reporter": "^6.0.0",
    "cucumberjs-junitxml": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-cucumber": "^1.4.0",
    "lodash": "^4.17.19",
    "ts-node": "^8.6.2",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@cucumber/cucumber-expressions": "^16.1.2",
    "puppeteer": "^19.9.0",
    "url-join-ts": "^1.0.5"
  }
}
